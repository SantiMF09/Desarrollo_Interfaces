<Window x:Class="Inicio.Cancelar_Compra"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Inicio"
        mc:Ignorable="d"
        Title="Cancelar_Compra" Height="150" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="125*"/>
            <ColumnDefinition Width="125*"/>
            <ColumnDefinition Width="125*"/>
            <ColumnDefinition Width="125*"/>
        </Grid.ColumnDefinitions>
        <Label Content="¿Desea cancelar su entrada?" HorizontalAlignment="Left" Margin="39,10,0,0" VerticalAlignment="Top" Height="26" Width="171" Grid.ColumnSpan="2" Grid.Column="1"/>
        <Button Content="SI" HorizontalAlignment="Left" Margin="86,89,0,0" VerticalAlignment="Top" Width="75" Height="20" Grid.ColumnSpan="2" Click="Button_Click"/>
        <Button Content="No" HorizontalAlignment="Left" Margin="86,89,0,0" VerticalAlignment="Top" Width="75" Height="20" Grid.Column="2" Grid.ColumnSpan="2" Click="Button_Click_1"/>

    </Grid>
</Window>
